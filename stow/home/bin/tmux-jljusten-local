#!/usr/bin/env bash

host=`basename $0`
thost=${host//tmux-/}
host=jljusten-${thost//jljusten-/}
host=${host}.local
#echo $host

echo -ne '\033[22;0t'  # Save title on stack
echo -ne "\033]0;tmux $thost\007"
ssh -o ServerAliveInterval=30 -t $host tmux a
echo -ne '\033[23;0t'  # Restore title from stack
